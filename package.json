{
  "name": "n4nd0-ng-playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:welcome": "ng build @n4nd0/welcome",
    "build:tree-control": "ng build @n4nd0/tree-control",
    "build:layout": "ng build @n4nd0/app-layout",
    "prod": "run-s setup ng build --prod",
    "ci:libs": "run-s build:*",
    "test": "jest --config ./jest.config.js --json --outputFile=./coverage/jest-test-results.json",
    "test:watch": "yarn test --watch",
    "setup": "run-s build:* typedoc test",
    "lint": "eslint projects/** src/** --no-error-on-unmatched-pattern",
    "e2e": "ng e2e",
    "demo": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o dist/storybook",
    "typedoc": "typedoc --json ./projects/n4nd0/tree-control/typedoc.json --exclude **/*.spec.* ./projects/n4nd0/tree-control",
    "pwa": "http-server -S -C cert.pem -p 8080 -c-1 dist/n4nd0-ng-playground"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.3",
    "@angular/cdk": "~10.2.7",
    "@angular/common": "^10.2.3",
    "@angular/compiler": "^10.2.3",
    "@angular/core": "^10.2.3",
    "@angular/forms": "^10.2.3",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^10.2.3",
    "@angular/platform-browser-dynamic": "^10.2.3",
    "@angular/router": "^10.2.3",
    "@angular/service-worker": "^10.2.3",
    "core-js": "^3.8.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.20",
    "rxjs": "^6.5.3",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular-devkit/core": "^10.2.0",
    "@angular-eslint/builder": "0.8.0-beta.2",
    "@angular-eslint/eslint-plugin": "^0.8.0-beta.2",
    "@angular-eslint/eslint-plugin-template": "^0.8.0-beta.2",
    "@angular-eslint/schematics": "^0.8.0-beta.2",
    "@angular-eslint/template-parser": "^0.8.0-beta.2",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "^10.2.3",
    "@angular/language-service": "^10.2.3",
    "@babel/core": "^7.12.9",
    "@south-paw/storybook-addon-overview-angular": "^1.0.1",
    "@storybook/addon-actions": "^6.1.6",
    "@storybook/addon-jest": "^6.1.6",
    "@storybook/addon-knobs": "^6.1.6",
    "@storybook/addon-links": "^6.1.6",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^6.1.6",
    "@storybook/angular": "^6.1.6",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "autoprefixer": "10.0.2",
    "babel-loader": "^8.2.1",
    "codelyzer": "^6.0.1",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "2.22.1",
