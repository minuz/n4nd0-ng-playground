{
  "name": "n4nd0-ng-playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:welcome": "ng build @n4nd0/welcome",
    "build:tree-control": "ng build @n4nd0/tree-control",
    "build:layout": "ng build @n4nd0/app-layout",
    "prod": "run-s setup ng build --prod",
    "ci:libs": "run-s build:*",
    "test": "jest --config ./jest.config.js --json --outputFile=./coverage/jest-test-results.json",
    "test:watch": "yarn test --watch",
    "setup": "run-s build:* typedoc test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "demo": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o dist/storybook",
    "typedoc": "typedoc --json ./projects/n4nd0/tree-control/typedoc.json --exclude **/*.spec.* ./projects/n4nd0/tree-control",
    "pwa": "http-server -S -C cert.pem -p 8080 -c-1 dist/n4nd0-ng-playground"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.2",
    "@angular/cdk": "~8.1.1",
    "@angular/common": "^8.1.2",
    "@angular/compiler": "^8.1.2",
    "@angular/core": "^8.1.2",
    "@angular/forms": "^8.1.2",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "^8.1.2",
    "@angular/platform-browser-dynamic": "^8.1.2",
    "@angular/pwa": "^0.801.2",
    "@angular/router": "^8.1.2",
    "@angular/service-worker": "^8.1.2",
    "core-js": "^3.1.4",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.15",
    "rxjs": "^6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.0.4",
    "@angular-devkit/build-angular": "^0.801.2",
    "@angular-devkit/build-ng-packagr": "^0.801.2",
    "@angular-devkit/core": "^8.1.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "^8.1.2",
    "@angular/language-service": "^8.1.2",
    "@babel/core": "^7.5.5",
    "@south-paw/storybook-addon-overview-angular": "^0.0.5",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-jest": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-notes": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/angular": "^5.1.9",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "babel-loader": "^8.0.6",
    "codelyzer": "~5.1.0",
    "jest": "^24.8.0",
    "jest-html-reporter": "^2.5.0",
    "jest-junit": "^6.4.0",
    "jest-preset-angular": "^7.1.1",
    "ng-packagr": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "ts-jest": "^24.0.2",
    "ts-node": "~8.3.0",
    "tsickle": ">=0.36.0",
    "tslib": "^1.9.0",
    "tslint": "^5.18.0",
